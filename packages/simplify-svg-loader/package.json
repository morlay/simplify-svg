{
  "name": "simplify-svg-loader",
  "version": "3.1.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "loader-utils": "1.1.x",
    "simplify-svg": "^3.1.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/../../node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "tsx",
      "ts",
      "json",
      "jsx",
      "js"
    ],
    "modulePaths": [
      "<rootDir>/packages"
    ],
    "testRegex": ".*/__tests__/.+\\.(generator|test|spec)\\.(ts|tsx)$"
  }
}
