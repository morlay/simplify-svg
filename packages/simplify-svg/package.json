{
  "name": "simplify-svg",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "version": "3.1.1",
  "dependencies": {
    "@types/cheerio": "0.22.x",
    "@types/lodash": "4.14.x",
    "cheerio": "1.0.0-rc.2",
    "lodash": "4.17.x",
    "node-murmurhash": "2.1.x",
    "postcss": "6.0.x",
    "svg-points": "6.0.x",
    "svgpath": "2.2.x"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/../../node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "tsx",
      "ts",
      "json",
      "jsx",
      "js"
    ],
    "modulePaths": [
      "<rootDir>/packages"
    ],
    "testRegex": ".*/__tests__/.+\\.(generator|test|spec)\\.(ts|tsx)$"
  }
}
