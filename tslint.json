{
  "extends": [
    "tslint:latest"
  ],
  "rules": {
    "no-eval": false,
    "semicolon": false,
    "no-empty": false,
    "no-console": false,
    "max-line-length": false,
    "ordered-imports": false,
    "object-literal-key-quotes": false,
    "object-literal-sort-keys": false,
    "no-reference": false,
    "member-access": false,
    "max-classes-per-file": false
  }
}