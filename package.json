{
  "name": "simplify-svg",
  "version": "0.0.3",
  "main": "lib/index.js",
  "description": "Simplify SVG which is exported by Sketch/Illustrator etc.",
  "scripts": {
    "lint": "eslint src/",
    "ut": "mocha -R spec --compilers .:babel-register src/__tests__/*.spec.js",
    "build": "babel src/ --out-dir lib/",
    "test": "npm run lint && npm run ut"
  },
  "files": [
    "src/",
    "lib/"
  ],
  "dependencies": {
    "cheerio": "0.20.x",
    "lodash": "4.6.x",
    "svg-shapes": "1.8.x",
    "svgo": "0.6.x",
    "svgpath": "2.1.x"
  },
  "devDependencies": {
    "babel-cli": "6.6.x",
    "babel-core": "6.7.x",
    "babel-eslint": "5.0.x",
    "babel-plugin-transform-object-rest-spread": "6.6.x",
    "babel-preset-es2015": "6.6.x",
    "babel-register": "6.7.x",
    "chai": "3.5.x",
    "eslint": "2.2.x",
    "eslint-config-airbnb": "6.1.x",
    "mocha": "2.4.x"
  },
  "keywords": [
    "svg",
    "clean",
    "scale"
  ],
  "author": "Morlay",
  "license": "WTFPL",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/morlay/simplify-svg.git"
  },
  "bugs": {
    "url": "https://github.com/morlay/simplify-svg/issues"
  },
  "homepage": "https://github.com/morlay/simplify-svg#readme"
}
